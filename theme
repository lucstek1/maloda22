  function pageEvents(webPixelsManagerAPI) {
    webPixelsManagerAPI.publish("page_viewed");
  },
  "https://cdn.shopify.com/shopifycloud/web-pixels-manager/0.0.163/iife/browser.js"
);

</script>


  
